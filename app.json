{
  "expo": {
    "name": "valsoft-2",
    "slug": "valsoft-2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffaa"
      },
      "package": "com.valsoft.valsoftapp"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9f7ec43a-0874-41ea-b956-81a2df64f01a"
      }
    }
  }
}
